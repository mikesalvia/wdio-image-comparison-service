{
  "name": "wdio-image-comparison-service",
  "version": "1.5.1",
  "description": "Image comparison / visual regression testing for WebdriverIO",
  "main": "build/index.js",
  "scripts": {
    "clean": "rimraf coverage build .tmp",
    "compile": "babel lib/ --ignore **/*.spec.js -d build/",
    "lint": "eslint ./lib",
    "test.local.init": "wdio ./tests/configs/wdio.local.init.conf.js",
    "test.local.desktop": "wdio tests/configs/wdio.local.desktop.conf.js",
    "test.unit.coverage": "jest --coverage ",
    "test.saucelabs": "wdio ./tests/configs/wdio.saucelabs.conf.js",
    "watch": "npm run compile -- --watch",
    "release": "np",
    "precompile": "npm run clean",
    "prepublish": "npm run compile",
    "prerelease": "npm run compile"
  },
  "keywords": [
    "webdriverio",
    "visual",
    "regression",
    "image",
    "comparison"
  ],
  "author": "wswebcreation",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@wdio/cli": "^5.12.1",
    "@wdio/jasmine-framework": "^5.12.1",
    "@wdio/local-runner": "^5.12.1",
    "@wdio/sauce-service": "^5.12.1",
    "@wdio/selenium-standalone-service": "^5.12.1",
    "@wdio/spec-reporter": "^5.12.1",
    "@wdio/sync": "^5.12.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "fs-extra": "^7.0.1",
    "jest": "^24.9.0",
    "np": "^5.0.3",
    "rimraf": "^2.7.1",
    "webdriverio": "^5.12.1"
  },
  "dependencies": {
    "webdriver-image-comparison": "github:mikesalvia/webdriver-image-comparison#dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wswebcreation/wdio-image-comparison-service.git"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/lib/tests/*.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/",
      "<rootDir>/node_modules/"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "node_modules/",
      "tests/",
      "docs/"
    ]
  }
}
